page {
  display: block;
  min-height: 100%;
  background-color: #EFEFF4;
  padding-bottom: 100rpx;
}
.user-container {
  margin: 0
}

.list-box {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 30rpx 16rpx;
  position: relative;
}

.list-box .icon-tuan {
  position: absolute;
  right: 0;
  bottom: 0;
  display: inline-block;
  width: 68rpx;
  height: 61rpx;
}

.list-box .top {
  display: flex;
}

.list-box .top .avatar {
  display: inline-block;
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
}

.list-box .top .desc {
  flex: 1;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  align-content: center;
  justify-content: space-around;
  vertical-align: middle;
}

.list-box .top .desc .main {
  flex: 1;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.list-box .top .desc .main .name {
  font-family: PingFangSC-Medium;
  color: #4B4B4B;
  letter-spacing: 0;
  display: block;
  word-wrap: break-word;
  word-break: break-all;
  flex: 1;
  line-height: 40rpx;
}

.list-box .top .desc .main .price {
  font-family: PingFangSC-Medium;
  color: #FF6700;
  letter-spacing: 0;
  line-height: 40rpx;
}

.list-box .top .desc .title {
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #6E6E70;
  letter-spacing: 0;
}

.list-box .overview {
  margin-top: 30rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #6E6E70;
  letter-spacing: 0;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  max-height:84rpx;
  word-break:break-all;
}

.list-box .skills .skill {
  display: inline-block;
  color: #6e6e70;
  font-size: 24rpx;
  line-height: 33rpx;
  background: #eef1f8;
  padding: 11rpx 15rpx 8rpx 18rpx;
  border-radius: 60rpx;
  margin: 20rpx 20rpx 0 0;
}
.common-bar {
  height: 100rpx;
  border-top: 1px solid #efefef;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  display: flex;
  font-size: 22rpx;
  color: #6c6c71;
  width: 100%;
  box-sizing: border-box;
  padding: 0 50rpx;
}
.common-bar > view {
  flex: 1;
}
.common-bar image {
  display: block;
  margin: 15rpx auto 7rpx;
}
.common-bar text {
  display: block;
  text-align: center;
}
.project-bar image {
  width: 37rpx;
  height: 35rpx;
}
.freelancer-bar image {
  width: 43rpx;
  height: 34rpx;
  margin: 15rpx auto 7rpx;
}
.profile-bar image {
  width: 28rpx;
  height: 36rpx;
  margin: 14rpx auto 6rpx;
}
.tab-bar-color {
  color: #ff6700;
}

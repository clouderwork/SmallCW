page {
  display: block;
  min-height: 100%;
  background-color: #EFEFF4;
}

view {
    box-sizing: border-box;
}

.pro-money {
    margin-top: 20rpx;
    font-size: 30rpx;
}

.first-box {
    border-bottom: 2rpx solid #f4f4f4;
    display: flex;
    background: #fff;
    padding: 29rpx 19rpx;
    line-height: 26px;
}

.money-text {
    border: none;
    text-align: right;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    box-flex: 1;
    flex: 1;
    display: block;
    width: 100%;
    outline: none;
    margin-right: 8rpx;
}

.first-box-center {
    color: #cfcfcf;
}

.second-box {
    text-align: center;
    font-size: 30rpx;
    color: #ff6700;
    background: #fff;
    padding: 18rpx 19rpx;
}

.service-money {
    display: block;
    font-size: 24rpx;
    color: #9e9e9e;
    margin-top: 12rpx;
}

.third-box {
    margin-top: 20rpx;
    background: #fff;
    padding: 29rpx 19rpx;
    line-height: 26px;
}

.third-box-left {
    float: left;
}

.switch-role {
    background-color:#fff;
    line-height:100rpx;
    text-align:center;
    font-size:34rpx;
    color:#ff6700;
    margin-top:40rpx;
    border-top: 2rpx solid #e2e2e9;
    position: fixed;
    bottom:0;
    left: 0;
    right: 0;
}

.switch-disable {
    background-color:#e2e2e2;
    line-height:100rpx;
    text-align:center;
    font-size:34rpx;
    color:#9e9e9e;
    margin-top:40rpx;
    border-top: 2rpx solid #e2e2e9;
    position: fixed;
    bottom:0;
    left: 0;
    right: 0;
}

picker {
    float: right;
    background: url('images/icon_next.png') no-repeat right center;
    background-size: 14rpx 25rpx;
    height: 53rpx;
    line-height: 17rpx;
    padding-right: 30rpx;
}

.img-box {
    line-height: 40rpx;
}

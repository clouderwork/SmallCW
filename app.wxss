/**app.wxss**/
@import "/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.week-mask_transparent {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
}

.week-toast {
  position: fixed;
  z-index: 5000;
  width:270rpx;
  min-height:300rpx;
  top:180px;
  left:50%;
  margin-left:-150rpx;
  background:rgba(17, 17, 17, 0.6);
  text-align:center;
  border-radius:5px;
  color:#FFFFFF;
  padding:30rpx;
}

.icon_toast {
  margin: 22px 0 10px;
  display: block;
}

.week-toast__content {
  word-wrap: break-word;
  word-break: break-all;
  line-height: 1.2;
}

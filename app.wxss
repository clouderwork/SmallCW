/**app.wxss**/
@import "/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.week-mask_transparent {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
}

.week-toast {
  position: fixed;
  z-index: 5000;
  width:343rpx;
  min-height:243rpx;
  top:180px;
  left:50%;
  margin-left:-200rpx;
  background:rgba(48, 48, 48, 0.7);
  text-align:center;
  border-radius:5px;
  color:#FFFFFF;
  padding:30rpx;
  text-align: center;
}

.icon_toast {
  margin: 52rpx 130rpx 28rpx;
  display: inline-block;
  width: 75rpx;
  height:75rpx;
}

.week-toast__content {
  word-wrap: break-word;
  word-break: break-all;
  font-family:FZLANTY_XIANJW--GB1-0;
  font-size:30rpx;
  color:#fff;
  letter-spacing:0.63px;
  text-align:left;
}
